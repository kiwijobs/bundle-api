{
    "name": "kiwijobs/bundle-api",
    "description": "",
    "keywords": [],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.absolvent.pl"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "absolvent/bundle-swagger": "^1.8.1",
        "codecasts/laravel-jwt": "^0.8.5",
        "laravel/framework": "5.6.*|5.7.*|5.8.*",
        "league/fractal": "^0.15.0|^0.17.0",
        "league/uri": "^5.0",
        "riverline/multipart-parser": "^1.1",
        "symfony/yaml": "^3.2"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.4",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
            "Absolvent\\api\\": "app/",
            "Absolvent\\api\\database\\": "database/",
            "Absolvent\\api\\tests\\": "tests/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Absolvent\\api\\fixtures\\": "fixtures/"
        }
    },
    "scripts": {
        "build": [
            "composer install -o"
        ],
        "clean": [

        ],
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "sort-packages": true
    }
}
